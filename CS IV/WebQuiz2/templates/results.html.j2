{% block body %}

{% if rows|length == 0 %}
    <p>Zero results</p>
{% else %}
    <ul>
    {% for row in rows %}
        <li>{{row['email']}}</li>
    {% endfor %}
    </ul>
{% endif %}

{% endblock %}